<%= form_for @task, url: update_task_path(@task.id.to_s) do |f| -%>
    <%= f.hidden_field :id %>
    <%= f.text_field :name%>
    <%= f.text_field :description %>
    <%= f.text_field :expiry, id: 'datepicker'%>
    <%= f.submit "save" %>
<% end %>
